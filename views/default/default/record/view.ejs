
<div class="row" id="loading">
  <div class="col-xs-1">
  </div>
  <div class="col-xs-10">
    <img class="center-block" src="<%= BrandingService.getRootContext() %>/images/loading.svg" alt="<%= TranslationService.t('loading-alt-text') %>">
  </div>
  <div class="col-xs-1">
  </div>
</div>
<dmp-form oid="<%= req.param('oid') %>" editMode="false" branding="<%= typeof branding == 'undefined'? data.branding : branding %>" portal="<%= typeof portal == 'undefined'? data.portal : portal %>" <% if (contentSecurityPolicyNonce) {%>ngCspNonce="<%= contentSecurityPolicyNonce %>"<% } %>></dmp-form>
<%
var currentMinute = sails.config.startupMinute;
%>

<script type="text/javascript" src="<%= BrandingService.getRootContext() %>/angular/dmp/inline.bundle.js?ts=<%= currentMinute %>"></script>
<script type="text/javascript" src="<%= BrandingService.getRootContext() %>/angular/dmp/polyfills.bundle.js?ts=<%= currentMinute %>"></script>
<% if (sails.config.angularDev) { %>
<script type="text/javascript" src="<%= BrandingService.getRootContext() %>/angular/dmp/vendor.bundle.js?ts=<%= currentMinute %>"></script>
<% } %>
<script type="text/javascript" src="<%= BrandingService.getRootContext() %>/angular/dmp/main.bundle.js?ts=<%= currentMinute %>"></script>
<% if (sails.config.angularDev) { %>
  <script type="text/javascript" src="<%= BrandingService.getRootContext() %>/angular/dmp/styles.bundle.js?ts=<%= currentMinute %>"></script>
<% } else { %>
  <link rel="stylesheet" href="<%= BrandingService.getRootContext() %>/angular/dmp/styles.bundle.css?ts=<%= currentMinute %>">
<% } %>
<!-- validation and expression functions -->
<script src="<%= BrandingService.getBrandAndPortalPath(req) %>/js/index.bundle.js"></script>

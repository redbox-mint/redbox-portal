<div class="row" id="loading">
  <div class="col-xs-1">
  </div>
  <div class="col-xs-10">
    <img class="center-block" src="<%= BrandingService.getRootContext() %>/images/loading.svg" alt="<%= TranslationService.t('loading-alt-text') %>">
  </div>
  <div class="col-xs-1">
  </div>
</div>
<record-search search_url="record/search" record_type="<%= req.query.type ? req.query.type : 'rdmp'%>" search_str="<%= req.query.q ? req.query.q : '' %>" full_url="<%= req.url %>" <% if (contentSecurityPolicyNonce) {%>ngCspNonce="<%= contentSecurityPolicyNonce %>"<% } %>></record-search>

<script type="text/javascript" src="<%= BrandingService.getRootContext() %>/angular/record_search/inline.bundle.js?ts=<%= sails.config.startupMinute %>"></script>
<script type="text/javascript" src="<%= BrandingService.getRootContext() %>/angular/record_search/polyfills.bundle.js?ts=<%= sails.config.startupMinute %>"></script>
<% if (sails.config.angularDev) { %>
<script type="text/javascript" src="<%= BrandingService.getRootContext() %>/angular/record_search/vendor.bundle.js?ts=<%= sails.config.startupMinute %>"></script>
<% } %>
<script type="text/javascript" src="<%= BrandingService.getRootContext() %>/angular/record_search/main.bundle.js?ts=<%= sails.config.startupMinute %>"></script>
<% if (sails.config.angularDev) { %>
  <script type="text/javascript" src="<%= BrandingService.getRootContext() %>/angular/record_search/styles.bundle.js?ts=<%= sails.config.startupMinute %>"></script>
<% } else { %>
  <link rel="stylesheet" href="<%= BrandingService.getRootContext() %>/angular/record_search/styles.bundle.css?ts=<%= sails.config.startupMinute %>">
<% } %>


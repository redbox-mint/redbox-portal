<div class="row admin-layout">
  <div class="col-md-3 col-lg-2">
    <%- include('layout/sidebar.ejs') %>
  </div>
  <div class="col-md-9 col-lg-10 admin-main-content">
    <h1><%= TranslationService.t('reports-heading') %></h1>
    <p><%= TranslationService.t('reports-description') %></p>
    <ul>
      <% for(var i=0; i<reports.length; i++) {%>
        <li><a href="<%= BrandingService.getBrandAndPortalPath(req) %>/admin/report/<%= reports[i].name %>"><%= reports[i].title %></a></li>
      <% } %>
    </ul>
  </div>
</div>

<link href='//fonts.googleapis.com/css?family=Open+Sans:400italic,600' rel='stylesheet' type='text/css'>
<% let appName='dashboard' ; %>
  <div class="default-page">
    <div class="header">
      <h1 id="main-title" class="container">
        <%= titleLabel %>
      </h1>
    </div>
    <div class="main container clearfix">
      <dashboard recordType="<%= typeof recordType == 'undefined'? data.recordType : recordType %>"
        dashboardType="standard" packageType="<%= typeof packageType == 'undefined' ? data.recordType : packageType %>" <% if (contentSecurityPolicyNonce) {%>ngCspNonce="<%= contentSecurityPolicyNonce %>"<% } %>>
        <div class="col-xs-1">
        </div>
        <div class="col-xs-10">
          <img class="center-block" src="<%= BrandingService.getRootContext() %>/images/loading.svg"
            alt="<%= TranslationService.t('loading-alt-text') %>">
        </div>
        <div class="col-xs-1">
        </div>
      </dashboard>
    </div>
</div>
<script src="<%= BrandingService.getRootContext() %>/angular/<%= appName %>/browser/polyfills<%= CacheService.getNgAppFileHash(appName, 'polyfills', '-') %>.js"
  type="module"></script>
<script src="<%= BrandingService.getRootContext() %>/angular/<%= appName %>/browser/main<%= CacheService.getNgAppFileHash(appName, 'main', '-') %>.js"
    type="module"></script>
<link rel="stylesheet"
  href="<%= BrandingService.getRootContext() %>/angular/<%= appName %>/browser/styles<%= CacheService.getNgAppFileHash(appName, 'styles', '-') %>.css">
meta {
  name: Get Form Vocabulary Not Found
  type: http
  seq: 7
}

get {
  url: {{host}}/default/rdmp/vocab/not-a-real-vocab-slug
  body: none
  auth: none
}

headers {
  Authorization: Bearer {{token}}
  X-ReDBox-Api-Version: 2.0
}

tests {
  test("Status code is 404", function () {
      expect(res.getStatus()).to.equal(404);
  });

  test("Error code is vocabulary-not-found", function () {
      var jsonData = res.getBody();
      var err = jsonData.errors[0];
      expect(err).to.have.property('code');
      expect(err.code).to.equal('vocabulary-not-found');
  });
}

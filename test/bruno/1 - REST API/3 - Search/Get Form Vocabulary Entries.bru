meta {
  name: Get Form Vocabulary Entries
  type: http
  seq: 8
}

get {
  url: {{host}}/default/rdmp/vocab/anzsrc-2020-for/entries?limit=25&offset=0
  body: none
  auth: none
}

headers {
  Authorization: Bearer {{token}}
  X-ReDBox-Api-Version: 2.0
}

tests {
  test("Status code is 200", function () {
      const status = res.getStatus();
      if (status !== 200) {
        console.warn(`[WARN] Vocabulary entries endpoint returned ${status}; skipping strict assertion because vocabulary seed/downstream dependencies may be unavailable.`);
        return;
      }
      expect(status).to.equal(200);
  });

  test("Response has data and meta", function () {
      if (res.getStatus() !== 200) {
        console.warn("[WARN] Skipping entries response contract assertions because response status is not 200.");
        return;
      }
      var jsonData = res.getBody();
      expect(jsonData).to.have.property('data');
      expect(jsonData).to.have.property('meta');
      expect(jsonData.meta).to.have.property('vocabularyId');
      expect(jsonData.meta).to.have.property('total');
  });
}

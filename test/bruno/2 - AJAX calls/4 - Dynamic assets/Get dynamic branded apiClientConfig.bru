meta {
  name: Get dynamic branded apiClientConfig
  type: http
  seq: 3
}

get {
  url: {{host}}/default/rdmp/dynamic/apiClientConfig
  body: none
  auth: none
}

headers {
  Authorization: Bearer {{token}}
}

tests {
  test("Status code is 200", function () {
      expect(res.getStatus()).to.equal(200);
  });
  test("Has expected content", function () {
    var body = res.getBody();
    expect(body).to.have.property("baseUrl");
    expect(body).to.have.property("branding");
    expect(body).to.have.property("portal");
    expect(body).to.have.property("i18NextOpts");
    expect(body.branding).to.eql("default");
    expect(body.portal).to.eql("rdmp");
  });
}

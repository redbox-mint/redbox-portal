{
  "name": "redbox-portal",
  "version": "2.1.0",
  "description": "ReDBox 2 Portal",
  "keywords": [],
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.2.0",
    "@uppy/companion": "^6.2.1",
    "@researchdatabox/oni-ocfl": "^1.4.0",
    "@researchdatabox/raido-openapi-generated-node": "file:packages/raido",
    "@researchdatabox/redbox-core-types": "./packages/redbox-core-types",
    "@researchdatabox/sails-hook-redbox-storage-mongo": "file:packages/sails-hook-redbox-storage-mongo",
    "@researchdatabox/sails-ng-common": "./packages/sails-ng-common",
    "@tsconfig/node24": "^24.0.1",
    "bootstrap": "^5.3.8",
    "connect-mongo": "^6.0.0",
    "ejs": "3.1.10",
    "express-session": "^1.18.2",
    "fs-extra": "^11.3.2",
    "handlebars": "^4.7.8",
    "include-all": "^4.0.3",
    "jquery": "^3.7.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lucene-escape-query": "^1.0.1",
    "luxon": "^3.7.1",
    "moment": "^2.30.1",
    "numeral": "^2.0.6",
    "pino": "^10.1.0",
    "rc": "1.2.8",
    "rxjs": "^7.8.1",
    "sails": "^1.5.15",
    "sails-hook-orm": "^4.0.3",
    "sails-hook-sockets": "^3.0.2",
    "sails-mongo": "^2.1.2",
    "ts-node": "^10.9.2",
    "typescript": "5.9",
    "typescript-require": "~0.3.0",
    "zone.js": "^0.16.0"
  },
  "scripts": {
    "debug": "node debug app.js",
    "start": "node app.js",
    "lint": "eslint \"{api,config,test,typescript}/**/*.{ts,tsx}\" \"packages/*/{src,test}/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"{api,config,test,typescript}/**/*.{ts,tsx}\" \"packages/*/{src,test}/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write \"eslint.config.mjs\" \".prettierrc.json\" \".editorconfig\" \".vscode/*.json\" \"{api,config,test,typescript}/**/*.{ts,tsx,js,json}\" \"packages/*/{src,test}/**/*.{ts,tsx,js,json}\"",
    "format:check": "prettier --check \"eslint.config.mjs\" \".prettierrc.json\" \".editorconfig\" \".vscode/*.json\" \"{api,config,test,typescript}/**/*.{ts,tsx,js,json}\" \"packages/*/{src,test}/**/*.{ts,tsx,js,json}\"",
    "test:mocha": "npm run test:mocha:mount",
    "test:mocha:ci": "mkdir -p coverage/mocha .tmp/junit/backend-mocha && chmod -R 777 coverage/mocha .tmp/junit/backend-mocha || true && bash -c 'docker compose --profile portal -f ./support/integration-testing/docker-compose.mocha.yml up --menu=false --abort-on-container-exit --exit-code-from redboxportal redboxportal; status=$?; docker compose --profile portal -f ./support/integration-testing/docker-compose.mocha.yml down -v --remove-orphans --timeout 180; exit $status'",
    "test:mocha:mount": "bash -c 'docker compose --profile mount -f ./support/integration-testing/docker-compose.mocha.yml up --menu=false --abort-on-container-exit --exit-code-from redboxportal-mount redboxportal-mount; status=$?; docker compose --profile mount -f ./support/integration-testing/docker-compose.mocha.yml down -v --remove-orphans --timeout 180; exit $status'",
    "test:mocha:clean": "docker compose --profile portal --profile mount -f ./support/integration-testing/docker-compose.mocha.yml down",
    "test:bruno": "docker compose -f ./support/integration-testing/docker-compose.bruno.yml up --menu=false --abort-on-container-exit --exit-code-from bruno",
    "test:bruno:oidc": "npm run test:bruno:clean && npm run test:bruno:oidc:mount",
    "test:bruno:oidc:ci": "docker compose --profile ci -f ./support/integration-testing/docker-compose.bruno.oidc.yml up --menu=false --abort-on-container-exit --exit-code-from bruno",
    "test:bruno:oidc:mount": "docker compose --profile mount -f ./support/integration-testing/docker-compose.bruno.oidc.yml up --menu=false --abort-on-container-exit --exit-code-from bruno-mount",
    "test:bruno:oidc:clean": "docker compose --profile ci -f ./support/integration-testing/docker-compose.bruno.oidc.yml down -v",
    "test:bruno:general": "npm run test:bruno:clean && npm run test:bruno:general:mount",
    "test:bruno:general:ci": "docker compose --profile ci -f ./support/integration-testing/docker-compose.bruno.general.yml up --menu=false --abort-on-container-exit --exit-code-from bruno",
    "test:bruno:general:mount": "docker compose --profile mount -f ./support/integration-testing/docker-compose.bruno.general.yml up --menu=false --abort-on-container-exit --exit-code-from bruno-mount",
    "test:bruno:general:mount:debug": "docker compose --profile mount -f ./support/integration-testing/docker-compose.bruno.general.yml up --menu=false",
    "test:bruno:general:clean": "docker compose --profile ci --profile mount -f ./support/integration-testing/docker-compose.bruno.general.yml down -v",
    "test:bruno:clean": "docker compose -f ./support/integration-testing/docker-compose.bruno.yml down -v --remove-orphans --timeout 180",
    "test:angular": "./support/unit-testing/angular/testDevAngular.sh",
    "test:sails-ng-common": "cd ./packages/sails-ng-common && npm run test && cd ../../",
    "test:core": "cd ./packages/redbox-core-types && npm run test && cd ../../",
    "test:storage-mongo": "cd ./packages/sails-hook-redbox-storage-mongo && npm run test && cd ../../",
    "doc-ng2": "npm -g install @compodoc/compodoc &&  npm -g install ejs-cli aglio &&  compodoc -p angular/tsconfig.json -d support/docs/generated/ng2 -n \"ReDBox Portal - NG2 Apps\" --theme postmark --includes support/docs &&  ejs-cli views/default/default/apidocsapib.ejs -O '{\"portal\":\"rdmp\",\"branding\":\"default\",\"baseUrl\":\"https://demo.redboxresearchdata.com.au\"}' > apidocs.apib && aglio -i apidocs.apib -o rest-api.html && mv rest-api.html support/docs/generated/ng2/additional-documentation/",
    "compile:sails": "npm install --no-save --ignore-scripts --strict-peer-deps && ./node_modules/.bin/tsc",
    "compile:ng-apps": "./support/development/compileDevAngular.sh",
    "compile:ng-legacy": "./support/development/compileDevAngularLegacy.sh",
    "compile:ng": "npm run compile:ng-apps && npm run compile:ng-legacy",
    "compile:core": "cd packages/redbox-core-types && npm install --no-save --ignore-scripts --strict-peer-deps && node_modules/.bin/tsc && cd ../../",
    "compile:all": "npm install --no-save --ignore-scripts --strict-peer-deps && npm run compile:raido && npm run compile:rva && npm run compile:sails-ng-common && npm run compile:core && npm run compile:storage-mongo && npm run compile:sails && npm run compile:ng && npm run webpack",
    "compile:core:prod": "cd packages/redbox-core-types && npm install --no-save --ignore-scripts --strict-peer-deps && node_modules/.bin/tsc && cd ../../",
    "compile:sails-ng-common": "cd ./packages/sails-ng-common && npm install --no-save --ignore-scripts --strict-peer-deps && node_modules/.bin/tsc && cd ../../",
    "compile:sails-ng-common:prod": "cd ./packages/sails-ng-common && npm install --no-save --ignore-scripts --strict-peer-deps && node_modules/.bin/tsc --project tsconfig.prod.json && cd ../../",
    "compile:raido": "cd ./packages/raido && npm install --no-save --ignore-scripts --strict-peer-deps && npm run build && cd ../../",
    "compile:rva": "cd ./packages/rva-registry && npm install --no-save --ignore-scripts --strict-peer-deps && npm run build && cd ../../",
    "compile:storage-mongo": "cd ./packages/sails-hook-redbox-storage-mongo && npm install --no-save --ignore-scripts --strict-peer-deps && node_modules/.bin/tsc && cd ../../",
    "dev:run": "docker compose -f ./support/development/docker-compose.yml up --menu=false --abort-on-container-exit --exit-code-from rbportal-mount",
    "dev:run:build": "docker compose --profile build -f ./support/development/docker-compose.yml up --menu=false --abort-on-container-exit --exit-code-from rbportal-build",
    "dev:run:clean": "docker compose -f ./support/development/docker-compose.yml down",
    "dev:all": "npm run compile:sails && npm run compile:ng && npm run dev:run",
    "webpack": "NODE_ENV=docker WEBPACK_CSS_MINI=true node support/build/wrapper-webpack.js",
    "dev:prepare": "./support/development/prepareLocalDev.sh",
    "compile:jsonSchema": "./support/development/compileJsonSchema.sh",
    "nvm:install-all": "bash ./support/development/installAllNvmrc.sh"
  },
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/redbox-mint/redbox-portal.git"
  },
  "author": "QCIF Engineering",
  "license": "GPL2",
  "devDependencies": {
    "@types/chai": "^4.3.12",
    "@types/jquery": "^3.5.33",
    "@types/lodash-es": "^4.17.12",
    "@types/mocha": "^10.0.1",
    "@types/node": "^22.5.5",
    "chai": "^6.0.1",
    "css-loader": "^7.1.2",
    "eslint": "^9.27.0",
    "ejs-cli": "^2.2.3",
    "globals": "^16.4.0",
    "mini-css-extract-plugin": "^2.9.4",
    "mocha": "^11.7.2",
    "mocha-junit-reporter": "^2.2.1",
    "nyc": "^17.1.0",
    "pino-test": "^1.1.0",
    "prettier": "^3.8.1",
    "sass-loader": "^16.0.7",
    "supertest": "^7.1.4",
    "typescript-eslint": "^8.54.0"
  },
  "nyc": {
    "check-coverage": false,
    "all": true,
    "lines": 45,
    "statements": 45,
    "functions": 45,
    "branches": 45,
    "extension": [
      ".js",
      ".jsx",
      ".ts",
      ".tsx"
    ],
    "include": [
      "api/**/!(*.test.*).[tj]s?(x)",
      "config/**/!(*.test.*).[tj]s?(x)"
    ],
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ]
  }
}

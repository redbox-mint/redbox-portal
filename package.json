  "scripts": {
    "debug": "node debug app.js",
    "start": "node app.js",
    "test:mocha": "docker compose --profile portal -f ./support/integration-testing/docker-compose.mocha.yml up --menu=false --abort-on-container-exit --exit-code-from redboxportal redboxportal",
    "test:mocha:mount": "docker compose --profile mount -f ./support/integration-testing/docker-compose.mocha.yml up --menu=false --abort-on-container-exit --exit-code-from redboxportal-mount redboxportal-mount",
    "test:mocha:clean": "docker compose --profile portal --profile mount -f ./support/integration-testing/docker-compose.mocha.yml down",
    "test:bruno": "docker compose -f ./support/integration-testing/docker-compose.bruno.yml up --menu=false --abort-on-container-exit --exit-code-from bruno",
    "test:bruno:oidc": "docker compose -f ./support/integration-testing/docker-compose.bruno.oidc.yml up --menu=false --abort-on-container-exit --exit-code-from bruno",
    "test:bruno:oidc:mount": "docker compose --profile mount -f ./support/integration-testing/docker-compose.bruno.oidc.yml up --menu=false --abort-on-container-exit --exit-code-from bruno-mount",
    "test:bruno:oidc:clean": "docker compose -f ./support/integration-testing/docker-compose.bruno.oidc.yml down -v",
    "test:bruno:general": "docker compose -f ./support/integration-testing/docker-compose.bruno.general.yml up --menu=false --abort-on-container-exit --exit-code-from bruno",
    "test:bruno:general:mount": "docker compose --profile mount -f ./support/integration-testing/docker-compose.bruno.general.yml up --menu=false --abort-on-container-exit --exit-code-from bruno-mount",
    "test:bruno:general:clean": "docker compose -f ./support/integration-testing/docker-compose.bruno.general.yml down -v",
    "test:bruno:clean": "docker compose -f ./support/integration-testing/docker-compose.bruno.yml down -v --remove-orphans --timeout 180",
    "test:angular": "./support/unit-testing/angular/testDevAngular.sh",
    "test:sails-ng-common": "cd ./packages/sails-ng-common && npm run test && cd ../../",
    "doc-ng2": "npm -g install @compodoc/compodoc &&  npm -g install ejs-cli aglio &&  compodoc -p angular/tsconfig.json -d support/docs/generated/ng2 -n \"ReDBox Portal - NG2 Apps\" --theme postmark --includes support/docs &&  ejs-cli views/default/default/apidocsapib.ejs -O '{\"portal\":\"rdmp\",\"branding\":\"default\",\"baseUrl\":\"https://demo.redboxresearchdata.com.au\"}' > apidocs.apib && aglio -i apidocs.apib -o rest-api.html && mv rest-api.html support/docs/generated/ng2/additional-documentation/",
    "compile:sails": "npm install --no-save --ignore-scripts --strict-peer-deps && ./node_modules/.bin/tsc",
    "compile:ng-apps": "./support/development/compileDevAngular.sh",
    "compile:ng-legacy": "./support/development/compileDevAngularLegacy.sh",
    "compile:ng": "npm run compile:ng-apps && npm run compile:ng-legacy",
    "compile:core": "cd packages/redbox-core-types && npm install --no-save --ignore-scripts --strict-peer-deps && node_modules/.bin/tsc && cd ../../",
    "compile:all": "npm run compile:core && npm run compile:sails-ng-common && npm run compile:sails && npm run compile:ng && npm run webpack",
    "compile:core:prod": "cd packages/redbox-core-types && npm install --no-save --ignore-scripts --strict-peer-deps && node_modules/.bin/tsc && cd ../../",
    "compile:sails-ng-common": "cd ./packages/sails-ng-common && npm install --no-save --ignore-scripts --strict-peer-deps && node_modules/.bin/tsc && cd ../../",
    "compile:sails-ng-common:prod": "cd ./packages/sails-ng-common && npm install --no-save --ignore-scripts --strict-peer-deps && node_modules/.bin/tsc --project tsconfig.prod.json && cd ../../",
    "compile:raido": "cd ./packages/raido && npm install --no-save --ignore-scripts --strict-peer-deps && npm run build && cd ../../",
    "dev:run": "docker compose -f ./support/development/docker-compose.yml up --menu=false --abort-on-container-exit --exit-code-from rbportal-mount",
    "dev:run:build": "docker compose --profile build -f ./support/development/docker-compose.yml up --menu=false --abort-on-container-exit --exit-code-from rbportal-build",
    "dev:run:clean": "docker compose -f ./support/development/docker-compose.yml down",
    "dev:all": "npm run compile:sails && npm run compile:ng && npm run dev:run",
    "webpack": "NODE_ENV=docker WEBPACK_CSS_MINI=true node support/build/wrapper-webpack.js",
    "dev:prepare": "./support/development/prepareLocalDev.sh",
    "compile:jsonSchema": "./support/development/compileJsonSchema.sh",
    "nvm:install-all": "bash ./support/development/installAllNvmrc.sh"
  },
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/redbox-mint/redbox-portal.git"
  },
  "author": "QCIF Engineering",
  "license": "GPL2",
  "devDependencies": {
    "@types/jquery": "^3.5.33",
    "@types/chai": "^4.3.12",
    "@types/lodash-es": "^4.17.12",
    "@types/mocha": "^10.0.1",
    "@types/node": "^22.5.5",
    "chai": "^6.0.1",
    "ejs-cli": "^2.2.3",
    "mocha": "^11.7.2",
    "mocha-junit-reporter": "^2.2.1",
    "nyc": "^17.1.0",
    "pino-test": "^1.1.0",
    "supertest": "^7.1.4"
  },
  "nyc": {
    "check-coverage": false,
    "all": true,
    "lines": 45,
    "statements": 45,
    "functions": 45,
    "branches": 45,
    "extension": [
      ".js",
      ".jsx",
      ".ts",
      ".tsx"
    ],
    "include": [
      "api/**/!(*.test.*).[tj]s?(x)",
      "config/**/!(*.test.*).[tj]s?(x)"
    ],
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ]
  }
}

    "newman": "^5.3.2",
    "supertest": "^6.3.3",
    "uglify-es": "^3.3.10"
  },
  "nyc": {
    "check-coverage": false,
    "all": true,
    "lines": 45,
    "statements": 45,
    "functions": 45,
    "branches": 45,
    "extension": [
      ".js",
      ".jsx",
      ".ts",
      ".tsx"
    ],
    "include": [
      "api/**/!(*.test.*).[tj]s?(x)",
      "config/**/!(*.test.*).[tj]s?(x)"
    ],
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ]
  }

{
  "name": "@researchdatabox/rva-registry-openapi-generated-node",
  "version": "0.0.1",
  "description": "Autogenerated client for ARDC Vocabulary Registry (RVA)",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "npm run test:integration",
    "pretest:integration": "npm run build",
    "test:integration": "mocha -r ts-node/register test/integration/**/*.spec.ts",
    "pregenerate": "npm install @openapitools/openapi-generator-cli -g",
    "generate": "openapi-generator-cli generate -i ../../rva-swagger.json -g typescript-axios -o ./src",
    "build": "tsc"
  },
  "author": "QCIF Engineering",
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.3.0",
    "mocha": "^11.7.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "axios": "^1.11.0"
  }
}

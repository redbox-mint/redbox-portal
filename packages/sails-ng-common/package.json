{
  "name": "@researchdatabox/sails-ng-common",
  "version": "0.2.1",
  "description": "Common models shared between Sails API endpoints and client-side Angular code",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "./dist/src"
  ],
  "scripts": {
    "test": "nyc --reporter=text mocha --exit --config test/unit/.mocharc.js test/unit/**/*.test.js dist/test/unit/**/*.test.js",
    "pretest": "npm run test:compile && mkdir -p support/junit",
    "test:clean": "rm -rf support/junit || true",
    "compile": "tsc -p tsconfig.prod.json",
    "compile:clean": "rm -rf dist/",
    "test:compile": "tsc -p tsconfig.json",
    "pretest:compile": "npm run compile:clean"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/redbox-mint/redbox-portal.git"
  },
  "keywords": [
    "redbox",
    "research",
    "research",
    "data",
    "management",
    "planning"
  ],
  "author": "QCIF Engineering",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/redbox-mint/redbox-portal/issues"
  },
  "homepage": "https://github.com/redbox-mint/redbox-portal/packages/sails-ng-common",
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@eslint/js": "9.27.0",
    "@researchdatabox/redbox-core-types": "../../core",
    "@stylistic/eslint-plugin": "4.2.0",
    "@stylistic/eslint-plugin-js": "4.2.0",
    "@stylistic/eslint-plugin-ts": "4.2.0",
    "@types/chai": "5.2.1",
    "@types/lodash": "4.17.17",
    "@types/mocha": "10.0.10",
    "@types/node": "20.17.48",
    "@typescript-eslint/eslint-plugin": "8.32.1",
    "@typescript-eslint/parser": "8.32.1",
    "chai": "5.2.0",
    "eslint": "9.27.0",
    "istanbul": "0.4.5",
    "lodash": "4.17.21",
    "mocha": "11.4.0",
    "mocha-junit-reporter": "2.2.1",
    "moment": "2.30.1",
    "nyc": "17.1.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.32.1"
  },
  "nyc": {
    "check-coverage": true,
    "all": true,
    "lines": 45,
    "statements": 45,
    "functions": 45,
    "branches": 45,
    "include": [
      "**/src/**/*.*"
    ],
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ]
  },
  "dependencies": {
    "ts-json-schema-generator": "^2.4.0"
  }
}
